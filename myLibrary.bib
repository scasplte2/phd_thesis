@article{Jones2006,
abstract = {Photoassociation is the process in which two colliding atoms absorb a photon to form an excited molecule. The development of laser-cooling techniques for producing gases at ultracold ($\backslash$textless1 mK) temperatures allows photoassociation spectroscopy to be performed with very high spectral resolution. Of particular interest is the investigation of molecular states whose properties can be related, with high precision, to the properties of their constituent atoms with the “complications” of chemical binding accounted for by a few parameters. These include bound long-range or purely long-range vibrational states in which two atoms spend most or all of their time at large internuclear separations. Low-energy atomic scattering states also share this characteristic. Photoassociation techniques have made important contributions to the study of all of these. This review describes what is special about photoassociation spectroscopy at ultracold temperatures, how it is performed, and a sampling of results including the determination of scattering lengths, their control via optical Feshbach resonances, precision determinations of atomic lifetimes from molecular spectra, limits on photoassociation rates in a Bose-Einstein condensate, and briefly, production of cold molecules. Discussions are illustrated with examples on alkali-metal atoms as well as other species. Progress in the field is already past the point where this review can be exhaustive, but an introduction is provided on the capabilities of photoassociation spectroscopy and the techniques presently in use.},
author = {Jones, Kevin M. and Tiesinga, Eite and Lett, Paul D. and Julienne, Paul S.},
doi = {10.1103/RevModPhys.78.483},
file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2006 - Jones et al. - Ultracold photoassociation spectroscopy Long-range molecules and atomic scattering.pdf:pdf},
isbn = {0034-6861 1539-0756},
issn = {00346861},
journal = {Reviews of Modern Physics},
month = {may},
number = {2},
pages = {483--535},
pmid = {238699500005},
publisher = {American Physical Society},
title = {{Ultracold photoassociation spectroscopy: Long-range molecules and atomic scattering}},
url = {http://journals.aps.org/rmp/abstract/10.1103/RevModPhys.78.483},
volume = {78},
year = {2006}
}
@article{Nicholson2015a,
abstract = {Optical Feshbach resonances (OFRs) have generated significant experimental interest in recent years. These resonances are promising for many-body physics experiments, yet the practical application of OFRs has been limited. The theory of OFRs has been based on an approximate model that fails in important detuning regimes, and the incomplete theoretical understanding of this effect has hindered OFR experiments. We present the most complete theoretical treatment of OFRs to date, demonstrating important characteristics that must be considered in OFR experiments and comparing OFRs to the well-studied case of magnetic Feshbach resonances. We also present a comprehensive treatment of the approximate OFR model, including a study of the range of validity for this model. Finally, we derive experimentally useful expressions that can be applied to real experimental data to extract important information about the resonance structure of colliding atoms.},
annote = {Yellow - references
Purple - Important approximations or limitations
Red - Main points or sections

