eprint = {1308.3696},
file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2014 - Makotyn et al. - Universal dynamics of a degenerate unitary Bose gas.pdf:pdf},
isbn = {1745-2473},
issn = {17452473},
journal = {Nature Physics},
month = {jan},
number = {2},
pages = {116--119},
pmid = {17678142},
publisher = {Nature Research},
title = {{Universal dynamics of a degenerate unitary Bose gas}},
url = {http://www.nature.com/doifinder/10.1038/nphys2850},
volume = {10},
year = {2014}
}
@misc{XavierAntoine,
