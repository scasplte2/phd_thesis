file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2008 - J{\"{o}}rdens et al. - A Mott insulator of fermionic atoms in an optical lattice.pdf:pdf},
isbn = {0028-0836},
