issn = {10797114},
journal = {Physical Review Letters},
month = {may},
number = {8},
pages = {1--5},
pmid = {25768762},
title = {{Disorder-induced localization in a strongly correlated atomic Hubbard gas}},
url = {http://arxiv.org/abs/1305.6072v3},
volume = {114},
year = {2015}
}
