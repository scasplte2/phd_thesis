issn = {10502947},
journal = {Physical Review A},
number = {1},
pages = {132--142},
pmid = {9909581},
title = {{Continuous position measurements and the quantum Zeno effect}},
url = {http://journals.aps.org/pra/abstract/10.1103/PhysRevA.48.132},
volume = {48},
year = {1993}
}
