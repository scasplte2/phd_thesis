file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2006 - Naidon, Masnou-Seeuws - Photoassociation and optical Feshbach resonances in an atomic Bose-Einstein condensate Treatment of corre.pdf:pdf},
issn = {1050-2947},
journal = {Physical Review A},
keywords = {BEC,OFR,photoassociation,quench},
mendeley-tags = {BEC,OFR,photoassociation,quench},
month = {apr},
number = {4},
pages = {43611},
primaryClass = {cond-mat},
publisher = {American Physical Society},
title = {{Photoassociation and optical Feshbach resonances in an atomic Bose-Einstein condensate: Treatment of correlation effects}},
