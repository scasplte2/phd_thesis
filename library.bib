journal = {Journal of Materials Processing Technology},
keywords = {Laser implantation,Pulsed laser dispersing,Roll texturing,Skin-pass rolling},
month = {apr},
pages = {84--92},
pmid = {2406666115306510087},
title = {{Texturing of skin-pass rolls by pulsed laser dispersing}},
url = {http://arxiv.org/abs/1404.1100},
volume = {225},
