issn = {00104655},
journal = {Computer Physics Communications},
keywords = {Bose-Einstein condensate,Constrained Newton method,Deal.II,Distributed memory machines,FEM,Gross-PitaevskII equation,Stationary and non-stationary states,Topological modes},
month = {jun},
pages = {216--232},
title = {{ATUS-PRO: A FEM-based solver for the time-dependent and stationary Gross-Pitaevskii equation}},
url = {http://arxiv.org/abs/1506.07710},
volume = {202},
year = {2016}
}
