journal = {Physical Review Letters},
month = {nov},
number = {21},
pages = {4462--4465},
pmid = {11082571},
publisher = {American Physical Society},
title = {{Observation of optically induced Feshbach resonances in collisions of cold atoms}},
url = {http://link.aps.org/doi/10.1103/PhysRevLett.85.4462},
volume = {85},
year = {2000}
}
