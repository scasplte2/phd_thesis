file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2015 - Weber et al. - Mesoscopic Rydberg-blockaded ensembles in the superatom regime and beyond.pdf:pdf},
isbn = {1745-2473},
