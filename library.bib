@article{Casimir1948,
abstract = {The influence of retardation on the energy of interaction between two neutral atoms is investigated by means of quantum electrodynamics. As a preliminary step, Part I contains a discussion of the interaction between a neutral atom and a perfectly conducting plane, and it is found that the influence of retardation leads to a reduction of the interaction energy by a correction factor which decreases monotonically with increasing distance R. This factor is equal to unity for R small compared with the wave-lengths corresponding to the atomic frequencies, and is proportional to R-1 for distances large compared with these wave-lengths. In the latter case the total interaction energy is given by -3ℏc{\$}\alpha{\$}/8{\$}\pi{\$}R4, where {\$}\alpha{\$} is the static polarizability of the atom. Although the problem of the interaction of two atoms discussed in Part II is much more difficult to handle mathematically, the results are very similar. Again the influence of retardation can be described by a monotonically decreasing correction factor which is equal to unity for small distances and proportional to R-1 for large distances. In the latter case the energy of interaction is found to be -23ℏc{\$}\alpha{\$}1{\$}\alpha{\$}2/4{\$}\pi{\$}R7.},
author = {Casimir, H B G and Polder, D},
journal = {Physical Review},
month = {feb},
number = {4},
pages = {360},
title = {{The Influence of Retardation on the London-van der Waals Forces}},
url = {http://link.aps.org.ezp-prod1.hul.harvard.edu/doi/10.1103/PhysRev.73.360 http://prola.aps.org.ezp-prod1.hul.harvard.edu/abstract/PR/v73/i4/p360{\_}1 http://prola.aps.org.ezp-prod1.hul.harvard.edu/pdf/PR/v73/i4/p360{\_}1},
