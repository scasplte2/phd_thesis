@article{Javanainen2002,
abstract = {OBJECTIVE: The authors relate prehospital delay and in-hospital delay to the incidence of perforation of appendicitis.$\backslash$n$\backslash$nSUMMARY BACKGROUND DATA: Quality assurance studies use perforation rate as an index of quality of care. This is based on the assumption commonly presented in retrospective reports that in-hospital delay to surgery influences the incidence of perforation. Only one limited study prospectively found that prehospital delay increased the perforation rate.$\backslash$n$\backslash$nMETHODS: During a 6-month period, 95 consecutive adults undergoing appendectomies at Foothills Hospital in Calgary, Alberta, were questioned as to onset and type of first symptom (i.e., epigastric discomfort, anorexia nervosa, vomiting, and abdominal pain). Time of emergency room (ER) arrival, surgery consultation, and operating room start were taken from the chart. Surgical and pathology reports were used to identify status of appendix (normal, inflamed, suppurative, gangrenous, perforated) and presence of abscess cavity. The status of appendix was related to prehospital and in-hospital delay to establish significance.$\backslash$n$\backslash$nRESULTS: There were 13 (14{\%}) normal, 67 (70{\%}) inflamed, and 15 (16{\%}) perforated appendices. Patients with perforated appendices waited 2.5 times longer before reporting to the ER, compared with patients with inflamed appendices (57 hours vs. 22 hours, p {\textless} 0.007). Once in the hospital, patients with perforated appendices were identified and treated faster than those with inflamed appendices (7 vs. 9 hours, p {\textless} 0.039). Analysis by ER physician was 3 hours whether the appendix was normal, inflamed, or perforated. Analysis by the surgeon was significantly shorter in patients with perforated appendices than patients with inflamed appendices (4 vs. 6 hours, p {\textless} 0.039).$\backslash$n$\backslash$nCONCLUSIONS: This prospective study identifies that delay in presentation accounts for the majority of perforated appendices. Clinical evaluation is effective for identifying patients with more advanced disease. Indiscriminate appendectomy as an attempt to decrease perforation is not supported by these data. Hospital perforation rates likely reflect patient factors, illness attitude, and access to medical care.},
archivePrefix = {arXiv},
arxivId = {arXiv:cond-mat/0108349},
author = {Javanainen, Juha and Mackie, Matt},
doi = {10.1103/PhysRevLett.88.090403},
eprint = {0108349},
file = {:E$\backslash$:/Dropbox (Personal)/Papers {\&} Reference/Mendeley/2002 - Javanainen, Mackie - Rate Limit for Photoassociation of a Bose-Einstein Condensate.pdf:pdf},
issn = {10797114},
journal = {Physical Review Letters},
month = {feb},
number = {9},
pages = {4},
pmid = {7717781},
primaryClass = {arXiv:cond-mat},
publisher = {American Physical Society},
title = {{Rate Limit for Photoassociation of a Bose-Einstein Condensate}},
