month = {apr},
pages = {5},
title = {{Ultracold atom-electron interaction: from two to many-body physics}},
url = {http://arxiv.org/abs/1404.5761v1 http://arxiv.org/abs/1404.5761{\%}0Ahttp://dx.doi.org/10.1038/ncomms5546},
year = {2014}
}
