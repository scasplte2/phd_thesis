journal = {New Journal of Physics},
keywords = {Bose-Einstein condensate,Rydberg,Rydberg dressing,many-body,quantum many-body physics},
mendeley-tags = {Rydberg,Rydberg dressing,many-body},
month = {dec},
pages = {63012},
